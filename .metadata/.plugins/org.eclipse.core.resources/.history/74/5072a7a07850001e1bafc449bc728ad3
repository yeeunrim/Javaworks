package objectarray.dogs;

public class DogArray {

	public static void main(String[] args) {
		//Dog 인스턴스 4개 생성를 생성해서 배열에 저장하고
		//정보를 출력하세요.
		Dog[] dog = new Dog[4];
		
		dog[0].name = "쿠키";
		dog[1].name = "초코";
		dog[2].name = "백구";
		dog[3].name = "딸기";
		dog[4].name = "두부";
		
		dog[0].type = "말티즈";
		dog[1].type = "푸들";
		dog[2].type = "시고르자브종";
		dog[3].type = "닥스훈트";
		dog[4].type = "말티즈";

		
		System.out.println("개의 이름은 :" + dog[0].getName() + ", " 
				+ "개의 종류은 :" + dog[0].getType());
		
		

	}

}
