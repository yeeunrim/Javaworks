package classes.methods;

public class Calculator {
	//메서드 정의 - 더하기, 나누기, 빼기, 곱하기
	//정수형 계산기
	public int add(int n1, int n2) {
		return n1 + n2;
	}
	
	public int sub(int n1, int n2) {
		return n1 - n2;
	}
	
	public int mul(int n1, int n2) {
		return n1 * n2;
	}
	
	public int div(int n1, int n2) {
		return n1 / n2;
	}
}
