package methods;

public class MathTest {

	public static void main(String[] args) {
		//내장 함수 - 수학(Math)
		
		//절대값(거리)
		int v1 = Math.abs(-10); 
		System.out.println("v1의 절대 값은 " + v1 + "이다.");
		
		//반올림값
		long v2 = Math.round(5.67);
		System.out.println("v2의 반올림 값은 " + v2 + "이다.");
		
		//버림값
		long v3 = Math.round(5.67);
		System.out.println("v3의 버림 값은 " + v3 + "이다.");

	}

}
