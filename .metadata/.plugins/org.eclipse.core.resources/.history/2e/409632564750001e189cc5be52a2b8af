package classes.constructor;

//참조 자료형 - Person 클래스 정의
public class Person {
	String name; //이름
	float height; //키
	float weight; //몸무게
	
	//기본 생성자
	public Person() {}
	
	//메서드
	public void showPersonInfo() {
		System.out.printf("이름은 %s, 키는 %.2fcm이며 몸무게는 %.2fkg이다.", 
				name, height, weight);
	}
}
