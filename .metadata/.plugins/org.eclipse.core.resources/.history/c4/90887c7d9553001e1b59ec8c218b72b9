package singleton.student;

public class Main {

	public static void main(String[] args) {
		School student1 = School.getInstance();
		
		Student lee = student1.createStudent();
		Student kim = student1.createStudent();
		Student rim = student1.createStudent();
		
		System.out.println("학번 : " + lee.getStudentNum());
		System.out.println("학번 : " + kim.getStudentNum());
		System.out.println("학번 : " + rim.getStudentNum());

	}

}
