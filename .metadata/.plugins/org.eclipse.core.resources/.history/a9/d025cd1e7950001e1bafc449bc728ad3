package objectarray.dogs;

public class DogArray {

	public static void main(String[] args) {
		//Dog 인스턴스 4개 생성를 생성해서 배열에 저장하고
		//정보를 출력하세요.
		Dog[] dog = new Dog[4];
		
		dog[0] = new Dog("쿠키", "말티즈");
		dog[1] = new Dog("두부", "말티즈");
		dog[2] = new Dog("초코", "푸들");
		dog[3] = new Dog("백구", "말티즈");
		dog[4] = new Dog("딸기", "닥스훈트");
		
		dog[0].getName();
		

	}

}
