package kr.or.iei.run;

import kr.or.iei.model.User;

public class UserTest {

	public static void main(String[] args) {
		//크기가 3인 배열 객체 생성
		User[] users = new User[3];
		
		//배열 생성
		User user1 = new User("abc99", "abc1234", "임예은");
		User user2 = new User("abc99", "abc1234", "임예은");
		User user3 = new User("abc99", "abc1234", "임예은");
		
		//배열에 저장
		users[0] = user1;
		users[1] = user2;
		users[2] = user3;
		
		//전체 출력
		for(int i=0; i<users.length; i++) {
			System.out.println(users[i]);
		}
		

	}

}
