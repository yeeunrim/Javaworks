package transport;

public class Main {

	public static void main(String[] args) {
		//사람 객체 생성
		Person p1 = new Person("임예은", 10000);
		Person p2 = new Peㄱson("이강인", 20000);
		
		//버스 객체 생성
		Bus bus100 = new Bus(100);
		
		p1.takeBus(bus100);
		p2.takeBus(bus100);
		
		p1.showPersonInfo();
		p2.showPersonInfo();
		bus100.showBusInfo();

	}

}
