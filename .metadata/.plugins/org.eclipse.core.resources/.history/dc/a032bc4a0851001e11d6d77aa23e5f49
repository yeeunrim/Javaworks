package kr.or.iei.model;

//회원(User)클래스 생성
public class User {
	//필드(속성)
	private String id;
	private String pwd;
	private String name;
	
	//set(), get() 함수 
	//1. id - 입력, 출력
	public void SetId(String id) { //입력하는 것은 반환이 아니기 때문에 void 사용
		this.id = id;
	}
	
	public String getId() {
		return id;
	}
	
	//2. pwd - 입력, 출력
	public void SetPwd(String pwd) { 
		this.pwd = pwd;
	}
	
	public String getPwd() { //반환값이 있기에 String을 사용
		return pwd;
	}
	
}
